{
  "Description": "{\"createdOn\":\"Mac\",\"createdBy\":\"Amplify\",\"createdWith\":\"12.10.1\",\"stackType\":\"auth-Cognito-UserPool-Groups\",\"metadata\":{}}",
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "env": {
      "Type": "String"
    },
    "AuthRoleArn": {
      "Type": "String"
    },
    "UnauthRoleArn": {
      "Type": "String"
    },
    "authsaigeapp16dc1619eUserPoolId": {
      "Type": "String",
      "Default": "authsaigeapp16dc1619eUserPoolId"
    },
    "authsaigeapp16dc1619eIdentityPoolId": {
      "Type": "String",
      "Default": "authsaigeapp16dc1619eIdentityPoolId"
    },
    "authsaigeapp16dc1619eAppClientID": {
      "Type": "String",
      "Default": "authsaigeapp16dc1619eAppClientID"
    },
    "authsaigeapp16dc1619eAppClientIDWeb": {
      "Type": "String",
      "Default": "authsaigeapp16dc1619eAppClientIDWeb"
    }
  },
  "Conditions": {
    "ShouldNotCreateEnvResources": {
      "Fn::Equals": [
        {
          "Ref": "env"
        },
        "NONE"
      ]
    }
  },
  "Resources": {
    "userdevGroup": {
      "Type": "AWS::Cognito::UserPoolGroup",
      "Properties": {
        "UserPoolId": {
          "Ref": "authsaigeapp16dc1619eUserPoolId"
        },
        "Description": "override success",
        "GroupName": "userdev",
        "Precedence": 1,
        "RoleArn": {
          "Fn::GetAtt": [
            "userdevGroupRole",
            "Arn"
          ]
        }
      }
    },
    "userdevGroupRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "",
              "Effect": "Allow",
              "Principal": {
                "Federated": "cognito-identity.amazonaws.com"
              },
              "Action": "sts:AssumeRoleWithWebIdentity",
              "Condition": {
                "StringEquals": {
                  "cognito-identity.amazonaws.com:aud": {
                    "Ref": "authsaigeapp16dc1619eIdentityPoolId"
                  }
                },
                "ForAnyValue:StringLike": {
                  "cognito-identity.amazonaws.com:amr": "authenticated"
                }
              }
            }
          ]
        },
        "RoleName": {
          "Fn::Join": [
            "",
            [
              {
                "Ref": "authsaigeapp16dc1619eUserPoolId"
              },
              "-userdevGroupRole"
            ]
          ]
        }
      }
    }
  },
  "Outputs": {
    "userdevGroupRole": {
      "Value": {
        "Fn::GetAtt": [
          "userdevGroupRole",
          "Arn"
        ]
      }
    }
  }
}